<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<!-- This is a fairly minimal config file; see console or logfile output for more options. -->
<config>

	<module name="network">
		<param name="inputNetworkFile" value="../../../../examples/scenarios/equil/network.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="../../../../examples/scenarios/equil/plans100trips.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="./output/example" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="10" />
		<param name="eventsFileFormat" value="xml" />
		<param name="snapshotFormat" value="" />
	</module>


	<module name="qsim">
		<param name="snapshotperiod" value="00:01:00" />
	</module>

	<module name="planCalcScore" >
		<param name="activityType_0"            value="dummy" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
	</module>

	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="ChangeExpBeta" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />
<!-- 		<param name="ModuleDisableAfterIteration_2" value="50" /> -->
	</module>


</config>
