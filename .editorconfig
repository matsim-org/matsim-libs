
root = true

[*]
charset = utf-8
end_of_line = lf


[*.java]
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 150


[*.{xml, xsd, dtd}]
max_line_length = off
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 4
