<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE config SYSTEM "./dtd/config_v1.dtd">

<config>
	
	<module name="network">
		<param name="inputNetworkFile" value="./test/scenarios/berlin/network.xml.gz" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="z:/0.plans_out.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="./output/wech" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="0" />
	</module>

	<module name="simulation">

		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="00:00:00" />
		
		<param name = "snapshotperiod"	value = "00:00:10"/>
 		<param name = "snapshotFormat"	value = "otfvis"/> 
	</module>

	<module name="planCalcScore">

		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />

		<param name="lateArrival" value="-0" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+0" />
		<param name="traveling" value="-6" />
		<param name="waiting" value="-0" />
		
		<param name="activityType_0"            value="home" /> <!-- home -->
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />
	
		<param name="activityType_1"            value="work" /> 
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="12:00:00" />
		<param name="activityMinimalDuration_1" value="08:00:00" />
	
	</module>
	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />
	</module>

	<module name="vis">
		<param name="logo"		value = "(c) 2006 TUB-VSP"/>
		<param name="delay"		value = "200"/>
		<param name="linkwidthfactor"	value = "15"/>
		<param name="shownodelabels"	value = "false"/>
		<param name="showlinklabels"  	value = "true"/>
	</module>	
	
</config>