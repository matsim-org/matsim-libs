<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<config>

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="numberOfThreads" value="0" />
	</module>

	<module name="network">
		<param name="inputNetworkFile" value="test/scenarios/equil/network.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="test/scenarios/equil/plans100.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="test/output/invalid" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="100" />
	</module>

	<module name="qsim">
		<param name = "flowCapacityFactor"	value = "0.10"/>
		<param name = "storageCapacityFactor"	value = "0.50"/>
	</module>

	<module name="strategy">
		<!-- the test configures its own strategy manager in the code. -->
	</module>

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />
		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-66" />
		<param name="waiting" value="-0" />

		<param name="activityType_0"            value="h" />
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />

		<param name="activityType_1"            value="w" />
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="01:00:00" />
		<param name="activityMinimalDuration_1" value="00:10:00" />
		<param name="activityOpeningTime_1"     value="05:30:00" />
		<param name="activityLatestStartTime_1" value="05:30:00" />
	</module>

</config>