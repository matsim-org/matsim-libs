--

CREATE FUNCTION wivsim_choose_random_object(kgskey character varying, dlm_objart character varying) RETURNS geometry
    LANGUAGE sql
    AS $_$

    SELECT
      dlm_obj.the_geom
    FROM
      kgs_2007,
      dlm_obj
    WHERE
      kgs_2007.kgs = $1 AND
      ST_Contains(kgs_2007.the_geom, dlm_obj.the_geom)
      AND dlm_obj.objart = $2
    ORDER BY random()
    LIMIT 1;

  $_$;


