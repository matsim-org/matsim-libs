<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings version="1.0"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              schemaLocation = "gexf.xsd">

<jxb:globalBindings>
   <jxb:javaType name="int"  xmlType="xs:positiveInteger"
                  parseMethod="jakarta.xml.bind.DatatypeConverter.parseInt"
                  printMethod="jakarta.xml.bind.DatatypeConverter.printInt"/>
</jxb:globalBindings>

<jxb:schemaBindings>
  <jxb:package name="playground.andreas.gexf" />
  <jxb:nameXmlTransform>
    <jxb:elementName prefix="XML" />
    <jxb:typeName prefix="XML"  />
    <jxb:modelGroupName prefix="XML" />
    <jxb:anonymousTypeName prefix="XML" />
  </jxb:nameXmlTransform>      
              
</jxb:schemaBindings>              
              
</jxb:bindings>


