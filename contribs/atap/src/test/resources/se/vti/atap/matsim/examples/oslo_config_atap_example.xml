<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">

<config>

	<module name="atap">	
		<param name="reduceLogging" value="true" />
		<!-- Otherwise using default configuration, see ATAPConfigGroup for options. -->
	</module>
	
	<module name="emulation">
		<!-- Using default configuration, see EmulationConfigGroup for options. -->
	</module>

	<!-- Configuration not specfic to ATAP below. -->

	<module name="global">
		<param name="randomSeed" value="4711" />
    	<param name="numberOfThreads" value="4" />
		<param name="coordinateSystem" value="epsg:32633" />
	</module>
 
	<module name="network" >
		<param name="inputNetworkFile" value="https://zenodo.org/records/17610274/files/oslo_network_atap_example.xml.gz?download=1" />	
	</module>
	
	<module name="plans">	
	    <param name="inputPlansFile" value="https://zenodo.org/records/17609947/files/oslo_population_atap_example.xml.gz?download=1" />
	</module>
	
 	<module name="controler">
		<param name="outputDirectory" value="./output/oslo" />
		<param name="writeEventsInterval" value="100" />
		<param name="lastIteration" value="1000"/>
		<param name="routingAlgorithmType" value="AStarLandmarks" />
	</module>

	<!-- Only settings used by ATAP. -->
	<module name="strategy" > 

		<parameterset type="strategysettings">
			<param name="strategyName" value="ReRoute"/>
			<param name="weight" value="0.1"/>
		</parameterset>
		
		<parameterset type="strategysettings">			
			<param name="strategyName" value="TimeAllocationMutator"/>
			<param name="weight" value="0.1"/>
			<param name="disableAfterIteration" value="70"/>
		</parameterset>
		
		<parameterset type="strategysettings">
			<param name="strategyName" value="SubtourModeChoice"/>
			<param name="weight" value="0.1"/>
			<param name="disableAfterIteration" value="70"/>
		</parameterset>
	</module>

	<module name="qsim">
		<param name="flowCapacityFactor" value="0.01"/>
		<param name="storageCapacityFactor" value="0.03"/>
		<param name="mainMode" value="car"/>
		<param name="endTime" value="30:00:00" /> 
	</module>


	<module name="planscalcroute">
        <param name="networkModes" value="car"/>

        <parameterset type="teleportedModeParameters">
            <param name="mode" value="bike"/>
            <param name="beelineDistanceFactor" value="1.1"/>
			<!-- 15 km/h / 3.6 ==  4.16 m/sek -->
            <param name="teleportedModeSpeed" value="4.16"/>
		</parameterset>

        <parameterset type="teleportedModeParameters">
            <param name="mode" value="walk"/>
            <param name="beelineDistanceFactor" value="1"/>
			<!-- 5 km/h / 3.6 ==  1.39 m/sek -->
            <param name="teleportedModeSpeed" value="1.39"/>
		</parameterset>

        <parameterset type="teleportedModeParameters">
            <param name="mode" value="other"/>
            <param name="beelineDistanceFactor" value="1.3"/>
            <param name="teleportedModeSpeed" value="0.8333333333"/>
		</parameterset>

        <parameterset type="teleportedModeParameters">
            <param name="mode" value="pt"/>
            <param name="beelineDistanceFactor" value="1.3"/>
			<!-- 20 km/h / 3.6 ==  5.56 m/sek -->
            <param name="teleportedModeSpeed" value="5.56"/>
		</parameterset>
	</module>

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="1.0" /> 
		<param name="marginalUtilityOfMoney" value="0.016" />
		<param name="lateArrival" value="-3.654" />
		<param name="earlyDeparture" value="-0.0" />
		<param name="performing" value="1.218" />
		<param name="waiting" value="-0" />
		

		<!-- taking parameters from berlin/config.xml -->
		<parameterset type="modeParams" >
			<param name="mode" value="car" />
			<param name="constant" value="-2.5" />
			<param name="marginalUtilityOfTraveling_util_hr" value="-0.1938" /> 
			<param name= "monetaryDistanceRate" value= "-0.001" />
		</parameterset>
		<parameterset type="modeParams" >
			<param name="constant" value="3.0" />
			<param name="mode" value="pt" />
			<param name="marginalUtilityOfTraveling_util_hr" value="0.0" />      
		</parameterset>      
		<parameterset type="modeParams" >
			<param name="mode" value="walk" />
			<param name="constant" value="-1.0" />
			<param name="marginalUtilityOfTraveling_util_hr" value="-1.209" />      
		</parameterset>
		<parameterset type="modeParams" >
			<param name="constant" value="-7" />
			<param name="mode" value="bike" /> 
			<param name="marginalUtilityOfTraveling_util_hr" value="-1.2714" />      
		</parameterset>   			

		<parameterset type="activityParams">
			<param name="activityType"    value="home" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="12:00:00" />
			<param name="minimalDuration" value="08:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="work" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="08:00:00" />
			<param name="minimalDuration" value="06:00:00" />
			<param name="openingTime"     value="07:00:00" />
			<param name="latestStartTime" value="09:00:00" />
			<param name="earliestEndTime" value="undefined" />
			<param name="closingTime"     value="18:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="school" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="06:00:00" />
			<param name="minimalDuration" value="05:00:00" />
			<param name="openingTime"     value="07:30:00" />
			<param name="latestStartTime" value="09:00:00" />
			<param name="closingTime"     value="17:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="visit" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="03:00:00" />
			<param name="minimalDuration" value="01:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="business" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="10:00:00" />
			<param name="minimalDuration" value="06:00:00" />
			<param name="openingTime"     value="07:00:00" />
			<param name="latestStartTime" value="15:00:00" />
			<param name="closingTime"     value="22:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="leisure" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="04:00:00" />
			<param name="minimalDuration" value="01:00:00" />
			<param name="openingTime"     value="06:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="other" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="02:00:00" />
			<param name="minimalDuration" value="01:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="service" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="02:00:00" />
			<param name="minimalDuration" value="01:00:00" />
			<param name="openingTime"     value="08:00:00" />
			<param name="latestStartTime" value="16:00:00" />
			<param name="closingTime"     value="20:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="shopping" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="01:00:00" />
			<param name="minimalDuration" value="00:15:00" />
			<param name="openingTime"     value="07:30:00" />
			<param name="latestStartTime" value="20:00:00" />
			<param name="closingTime"     value="23:00:00" />
		</parameterset>
		<parameterset type="activityParams">
			<param name="activityType"    value="delivery" />
			<param name="priority"        value="1" />
			<param name="typicalDuration" value="01:00:00" />
			<param name="minimalDuration" value="00:15:00" />
		</parameterset>
	</module>



</config>
