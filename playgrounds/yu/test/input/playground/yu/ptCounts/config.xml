<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<config>

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="coordinateSystem" value="Atlantis" />
	</module>

	<module name="transit">
		<param name="transitScheduleFile" value="test/input/playground/yu/ptCounts/transitSchedule.xml"/>
		<param name="vehiclesFile" value="test/input/playground/yu/ptCounts/vehicles.xml"/>
		<param name="transitModes" value="pt" />
	</module>
	
	<module name="network">
		<param name="inputNetworkFile" value="test/input/playground/yu/ptCounts/ptNet.xml"/>
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="test/input/playground/yu/ptCounts/ptPop.xml" />
	</module>

	<module name="scenario">
		<param name="useTransit" value="true" />
		<param name="useVehicles" value="true" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="test/input/playground/yu/ptCounts" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="10" />
	</module>

	<module name="qsim">
		<!-- "start/endTime" of MobSim (00:00:00 == take earliest activity time/ run as long as active vehicles exist) -->
		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="24:00:00" />
	
		<param name="flowCapacityFactor" value="0.1" />
		<param name="storageCapacityFactor" value="0.3" />
	
		<param name = "snapshotperiod"	value = "00:00:00"/> <!-- 5// 00:00:00 means NO snapshot writing -->
		<param name = "snapshotFormat"	value = "otfvis"/> <!-- netvis, googleearth, transims -->
	</module>

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />

		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-6" />
		<param name="waiting" value="-0" />
		
		<param name="activityType_0"            value="h" /> <!-- home -->
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />
	
		<param name="activityType_1"            value="w" /> <!-- work -->
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
		<param name="activityMinimalDuration_1" value="06:00:00" />
		<param name="activityOpeningTime_1"     value="07:00:00" />
		<param name="activityLatestStartTime_1" value="09:00:00" />
		<param name="activityEarliestEndTime_1" value="" />
		<param name="activityClosingTime_1"     value="18:00:00" />
	
		<!-- Experiment of Manuel -->
		<!--
		<param name="activityType_2"        	value="transitInteraction" />
		<param name="activityPriority_2"        value="1" />
		<param name="activityTypicalDuration_2" value="00:03:00" />-->
	</module>
	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="0" /> <!-- //5//   0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />
		<!--
		<param name="ModuleProbability_3" value="0.1" />
		<param name="Module_3" value="TimeAllocationMutator"/> for this, man needs to use own "StrategyManagerConfigLoader"
		-->
	</module>
	
	<module name="ptCounts">
		<param name="inputBoardCountsFile" value="test/input/playground/yu/ptCounts/board.xml" />
		<param name="inputAlightCountsFile" value="test/input/playground/yu/ptCounts/alight.xml" />
		<param name="inputOccupancyCountsFile" value="test/input/playground/yu/ptCounts/occupancy.xml" />
		<param name="outputformat" value="kml" />
		<param name="distanceFilter" value="300000" />
		<param name="countsScaleFactor" value="10" />
		<param name="distanceFilterCenterNode" value="11" />
	</module>
	
</config>
