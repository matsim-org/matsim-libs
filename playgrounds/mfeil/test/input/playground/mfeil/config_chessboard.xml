<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<config>

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="coordinateSystem" value="Atlantis" />
		<param name="numberOfThreads" value="2" />
	</module>

	<module name="network">
		<param name="inputNetworkFile" value="../matsim/test/scenarios/chessboard/network.xml" />
	</module>

	<module name="facilities">
		<param name="inputFacilitiesFile" value="../matsim/test/scenarios/chessboard/facilities.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="../matsim/test/scenarios/chessboard/plans.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="/workspace/playgrounds/mfeil/output/Test1" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="10" />
		<param name="routingAlgorithmType" value="AStarLandmarks" />
	</module>

	<module name="simulation">
		<!-- "start/endTime" of MobSim (00:00:00 == take earliest activity time/ run as long as active vehicles exist) -->
		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="00:00:00" />
	
		<param name = "snapshotperiod"	value = "00:00:00"/> <!-- 00:00:00 means NO snapshot writing -->
		<param name = "snapshotFormat"	value = "otfvis"/> <!-- netvis, googleearth, transims -->
	</module>
	

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />

		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-6" />
		<param name="waiting" value="-0" />
		
		<param name="activityType_0"            value="home" /> <!-- home -->
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />
	
		<param name="activityType_1"            value="work" /> <!-- work -->
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
		<param name="activityMinimalDuration_1" value="06:00:00" />
		<param name="activityOpeningTime_1"     value="08:00:00" />
		<param name="activityLatestStartTime_1" value="10:00:00" />
		<param name="activityEarliestEndTime_1" value="14:00:00" />
		<param name="activityClosingTime_1"     value="20:00:00" />
		
		<param name="activityType_2"            value="leisure" /> <!-- leisure -->
		<param name="activityPriority_2"        value="1" />
		<param name="activityTypicalDuration_2" value="03:00:00" />
		<param name="activityMinimalDuration_2" value="02:00:00" />
		<param name="activityOpeningTime_2"     value="16:00:00" />
		<param name="activityLatestStartTime_2" value="" />
		<param name="activityEarliestEndTime_2" value="" />
		<param name="activityClosingTime_2"     value="23:00:00" />
				
		<param name="activityType_3"            value="shopping" /> <!-- shopping -->
		<param name="activityPriority_3"        value="1" />
		<param name="activityTypicalDuration_3" value="02:00:00" />
		<param name="activityMinimalDuration_3" value="01:00:00" />
		<param name="activityOpeningTime_3"     value="08:00:00" />
		<param name="activityLatestStartTime_3" value="" />
		<param name="activityEarliestEndTime_3" value="" />
		<param name="activityClosingTime_3"     value="18:00:00" />
		
		<param name="activityType_4"            value="shopping_test" /> <!-- shopping -->
		<param name="activityPriority_4"        value="1" />
		<param name="activityTypicalDuration_4" value="02:00:00" />
		<param name="activityMinimalDuration_4" value="01:00:00" />
		<param name="activityOpeningTime_4"     value="10:00:00" />
		<param name="activityLatestStartTime_4" value="" />
		<param name="activityEarliestEndTime_4" value="" />
		<param name="activityClosingTime_4"     value="18:00:00" />
	</module>

	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->
		
		<param name="ModuleProbability_1" value="0.1" />
		<param name="Module_1" value="PlanomatX" />
		
		<param name="ModuleProbability_2" value="0.0" />
		<param name="Module_2" value="TimeOptimizer" />
		
		<param name="ModuleProbability_3" value="0.9" />
		<param name="Module_3" value="BestScore" />
		
		<param name="ModuleProbability_4" value="0.0" />
		<param name="Module_4" value="Recycling" />
		
		<param name="ModuleProbability_5" value="0.0" />
		<param name="Module_5" value="Planomat" />
		
		<param name="ModuleProbability_6" value="0.0" />
		<param name="Module_6" value="TimeModeChoicer" />
		
		<param name="ModuleProbability_7" value="0.0" />
		<param name="Module_7" value="ReRoute" />
		
		<param name="ModuleProbability_8" value="0.0" />
		<param name="Module_8" value="LocationChoice" />
		
		<param name="ModuleProbability_9" value="0.0" />
		<param name="Module_9" value="PlansVariator" />

	</module>
	
	<module name="locationchoice">
		<param name="constrained" value="true"/>
		<param name="maxRecursions" value="20"/>
	</module>
	
	
	<module name="planomat">
		<param name="possibleModes" value="car,pt,walk"/> 
		<param name="jgapmaxGenerations" value="100"/>
	</module>
	
	<!--<module name="parallelEventHandling">-->
  	<!--	<param name="numberOfThreads" value="1" />-->
	<!--</module>-->
	

</config>
