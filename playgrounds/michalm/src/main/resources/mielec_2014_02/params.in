inputDir                ./src/main/resources/mielec_2014_02/
netFile                  ${inputDir}/network.xml
plansFile                ${inputDir}/20.plans.xml.gz

#agents who will call a taxi instead of travelling by car (from 1% to 4% of the population)
taxiCustomersFile        ${inputDir}/taxiCustomers_4.0.txt

#taxicabs (25 of 50)
taxisFile                ${inputDir}/taxis-25.xml

onlineVehicleTracker     false



optimizer.paramsClass               playground.michalm.taxi.optimizer.rules.RuleBasedTaxiOptimizerParams
optimizer.id                        RULE_DSE_FF
optimizer.travelTimeSource          FREE_FLOW_SPEED
optimizer.goal                      DEMAND_SUPPLY_EQUIL
optimizer.nearestRequestsLimit      999
optimizer.nearestVehiclesLimit      999
optimizer.cellSize                  1000

scheduler.destinationKnown          false
scheduler.vehicleDiversion          false
scheduler.pickupDuration            120
scheduler.dropoffDuration           60

