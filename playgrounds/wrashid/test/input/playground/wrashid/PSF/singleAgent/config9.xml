<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd"
[
		<!ENTITY INBASE    "test/input/playground/wrashid/PSF/singleAgent">
		<!ENTITY OUTBASE    "./output/PSF/singleAgent">
	]

>
<config>

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="coordinateSystem" value="Atlantis" />
	</module>

	<module name="network">
		<param name="inputNetworkFile" value="network.xml" />
	</module>

	<module name="facilities">
		<param name="inputFacilitiesFile" value="facilities.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="plans2.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="./test/output/playground/wrashid/PSF/singleAgent/ScoringTst/testScoring1" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="0" />
		<param name= "writeEventsInterval" value="4" />
		<param name="mobsim" value="JDEQSim" />
	</module>

	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />

		<param name="lateArrival" value="-18" />
		<param name="earlyDeparture" value="-0" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-6" />
		<param name="waiting" value="-0" />
		
		<param name="activityType_0"            value="h" /> <!-- home -->
		<param name="activityPriority_0"        value="1" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityMinimalDuration_0" value="08:00:00" />
	
		<param name="activityType_1"            value="w" /> <!-- work -->
		<param name="activityPriority_1"        value="1" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
		<param name="activityMinimalDuration_1" value="06:00:00" />
		<param name="activityOpeningTime_1"     value="07:00:00" />
		<param name="activityLatestStartTime_1" value="09:00:00" />
		<param name="activityEarliestEndTime_1" value="" />
		<param name="activityClosingTime_1"     value="18:00:00" />
		
		<param name="activityType_2"            value="parkingDeparture" /> <!-- depart from parking -->
		<param name="activityPriority_2"        value="1" />
		<param name="activityTypicalDuration_2" value="00:01:00" />
		<param name="activityMinimalDuration_2" value="00:01:00" />
		
		<param name="activityType_3"            value="parkingArrival" /> <!-- arrival in parking -->
		<param name="activityPriority_3"        value="1" />
		<param name="activityTypicalDuration_3" value="00:01:00" />
		<param name="activityMinimalDuration_3" value="00:01:00" />
	</module>
	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.1" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />
		
		<param name="ModuleProbability_3" value="0.7" />
		<param name="Module_3" value="TimeAllocationMutator" />
		
		
		<param name="ModuleProbability_4" value="0.1" />
		<param name="Module_4" value="playground.wrashid.PSF.replanning.TemplatePlanStrategy" />
	</module>
	
	<module name="JDEQSim">
	</module>
	
	<module name="PSF">
		<param name="default.maxBatteryCapacity" value="36000000" /> <!-- in [J]  -->
		<param name="default.chargingPowerAtParking" value="3500" /> <!-- in [W]  -->
		<param name="main.chargingTimesOutputFilePath" value="&OUTBASE;/chargingLog.txt" />
		<param name="main.energyUsageStatistics" value="&OUTBASE;/energyUsageStatistics" />
	
	
		<param name="testingModeOn" value="false" /> <!-- this is the main switch for all testing values -->
		
	
		<param name="testing.energyConsumptionPerLink" value="100000" /> <!-- in Joule [J] -->
		<param name="testing.maxEnergyPriceWillingToPay" value="1" /> <!-- in utils per [J]  -->
		
		<param name="main.numberOfHubs" value="1" />
		<param name="testing.peakHourElectricityPrice" value="0.8" /> <!-- in utils/J peakHour: from 07:00 to 20:00  -->
		<param name="testing.lowTariffElectrictyPrice" value="0.3" /> <!-- in utils/J lowTariff: from 20:00 to 07:00  -->
		<param name="main.hubLinkMappingPath" value="&INBASE;/hubLinkMapping3.txt" />
		<param name="main.chargingPriceScalingFactor" value="1000" />
		<param name="main.chargingMode" value="earliestCharging" />
		<!-- <param name="main.chargingPriceBlurFactor" value="0.01" /> -->
		
		
	</module>

</config>
